        "unique_name": "definition_activity_01WT5XJ3X3PGI5jIP4XZAXzgYQgUvIECV5o",
        "name": "Execute Python Script",
        "title": "Determine requested device reset info",
        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": true,
          "display_name": "Determine requested device reset info",
          "script": "import json, sys\ninput = sys.argv[1]\nlower_first = input[0].lower() + input[1:]\nendpoint = ''.join(lower_first.split())",
          "script_arguments": [
            "$workflow.definition_workflow_01WT5XJ0DM2E52iF77mRxKuj7Pf9zBh3en0.input.variable_workflow_01WT5XJ0O3NCC72sntmbqgv6WuAEwYZeepd$"
          ],
          "script_queries": [
            {
              "script_query": "endpoint",
              "script_query_name": "endpoint ",
              "script_query_type": "string"
            }
          ],
          "skip_execution": false
        },
        "object_type": "definition_activity"
