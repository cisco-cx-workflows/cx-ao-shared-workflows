        "title": "Determine requested group type",
        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": true,
          "display_name": "Determine requested group type",
          "script": "import json, sys\ninput = sys.argv[1]\nlower_first = input[0].lower() + input[1:]\nendpoint = ''.join(lower_first.split())",
          "script_arguments": [
            "$workflow.definition_workflow_01WTCU4314C210xGgHtfTmxbF8UdFFOnEk9.input.variable_workflow_01WTCU43ACEJU3x5eFHy6XtQY4jBLLRYw0E$"
